{
    "servers": [
        {
            "port": 8081,
            "endpoint": [
                {
                    "path": "/:type",
                    "verb": "GET",
                    "mappings": [
                        {
                            "params": [
                                {
                                    "type": "EQUALS",
                                    "right": {
                                        "type": "PATH",
                                        "id": "type"
                                    },
                                    "left": {
                                        "type": "STRING",
                                        "value": "json"
                                    }
                                }
                            ],
                            "code": 201,
                            "content": {
                                "type": "JSON",
                                "data": {
                                    "message": "User created"
                                }
                            }
                        },
                        {
                            "params": [
                                {
                                    "type": "EQUALS",
                                    "right": {
                                        "type": "PATH",
                                        "id": "type"
                                    },
                                    "left": {
                                        "type": "STRING",
                                        "value": "file"
                                    }
                                }
                            ],
                            "content": {
                                "type": "FILE",
                                "data": {
                                    "path": "./examples/file_or_json_example.doppl.json"
                                }
                            }
                        }
                    ]
                }
            ]
        }
    ]
}
